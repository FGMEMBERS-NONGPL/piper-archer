<?xml version="1.0"?>

<PropertyList>

  <path>Instruments/vsi/vsi.ac</path>

  <animation>
    <type>material</type>
    <object-name>legend</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>0.02</red>
      <green>0.015</green>
      <blue>0.005</blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/glossy-black-plastic.png</texture>
    <object-name>case</object-name>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
  <interpolation>
    <entry>
      <ind>-2000.0</ind>
      <dep>-175</dep>
    </entry>
     <entry>
       <ind>2000.0</ind>
       <dep>175</dep>
     </entry>
    </interpolation>
   <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>
   </axis>
  </animation>

</PropertyList>
