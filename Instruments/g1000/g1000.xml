<?xml version="1.0"?>

<PropertyList>

  <path>g1000.ac</path>

  <animation>
    <type>material</type>
    <object-name>bezel</object-name>
    <emission>
      <red>0.2</red>
      <green>0.15</green>
      <blue>0.05</blue>
      <factor-prop>electrical/outputs/panel-lights/brightness</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>display</object-name>
    <emission>
      <red>1.0</red>
      <green>1.0</green>
      <blue>1.0</blue>
      <factor-prop>electrical/outputs/panel-lights/brightness</factor-prop>
    </emission>
  </animation>

</PropertyList>
