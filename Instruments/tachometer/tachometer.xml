<?xml version="1.0"?>

<PropertyList>

  <path>Instruments/tachometer/tachometer.ac</path>

  <animation>
    <type>material</type>
    <object-name>legend</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>0.2</red>
      <green>0.15</green>
      <blue>0.05</blue>
      <factor-prop>electrical/outputs/panel-lights/brightness</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/glossy-black-plastic.png</texture>
    <object-name>case</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine[0]/rpm</property>

    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>3500</ind>
        <dep>270</dep>
      </entry>
    </interpolation>
    
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    
  </animation>

</PropertyList>
