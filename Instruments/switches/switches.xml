<?xml version="1.0"?>

<PropertyList>

  <path>Instruments/switches/switches.ac</path>

  <animation>
    <type>material</type>
    <object-name>fuel pump</object-name>
    <object-name>landing lights</object-name>
    <emission>
      <red>0.02</red>
      <green>0.015</green>
      <blue>0.005</blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/glossy-black-plastic.png</texture>
    <object-name>backplate</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>fuel pump</object-name>
    <factor>-30</factor>
    <property>controls/switches/fuel-pump</property>
    <interpolation>
      <entry>
        <ind> 0.0</ind>
        <dep>-10</dep>
      </entry>
      <entry>
        <ind>  1.0</ind>
        <dep> 10.0</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
    <center>
      <x-m> 0.0  </x-m>
      <y-m> 0.0  </y-m>
      <z-m>-0.002</z-m>
    </center>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>fuel pump</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/fuel-pump</property>
      </binding>
    </action>
  </animation>

</PropertyList>
