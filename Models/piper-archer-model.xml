<?xml version="1.0"?>

<PropertyList>

  <!-- Export X/Y with Mirror col. as Ambient -->
  <path>piper-archer.ac</path>

  <!-- REMBRANDT -->

  <animation>
    <type>select</type>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>shadow catcher</object-name>
  </animation>

  <effect>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>prop disc front</object-name>
    <object-name>prop disc rear</object-name>
    <object-name>left landing light flare</object-name>
    <object-name>right landing light flare</object-name>
    <object-name>left navigation light flare</object-name>
    <object-name>right navigation light flare</object-name>
    <object-name>left strobe light flare</object-name>
    <object-name>right strobe light flare</object-name>
    <object-name>tail beacon light flare</object-name>
  </effect>

  <effect>
    <inherits-from>Models/Effects/fuselage</inherits-from>
    <object-name>fuselage</object-name>

    <object-name>left flap</object-name>
    <object-name>left flap arm</object-name>
    <object-name>right flap</object-name>
    <object-name>right flap arm</object-name>

    <object-name>left aileron</object-name>
    <object-name>right aileron</object-name>

    <object-name>rudder</object-name>

    <object-name>elevator</object-name>
    <object-name>elevator trim tab</object-name>

    <object-name>main gear fairing</object-name>

    <object-name>left main fairing</object-name>
    <object-name>right main fairing</object-name>
    <object-name>nose gear fairing</object-name>
  </effect>

  <!-- COCKPIT LIGHTING -->

  <animation>
    <type>material</type>
    <condition>
      <not>
        <property>/sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <object-name>glareshield</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>right yoke logo</object-name>

    <!-- <object-name>yoke rod</object-name> -->
    <!-- <object-name>yoke rod gasket</object-name> -->
    <!-- <object-name>left yoke</object-name> -->
    <!-- <object-name>right yoke</object-name> -->
    <emission>
      <red>0.3</red>
      <green>0.2</green>
      <blue>0.2</blue>
      <factor-prop>electrical/outputs/cockpit-lights/brightness</factor-prop>
    </emission>
  </animation>

  <!-- Rembrandt -->

  <animation>
    <type>light</type>
    <light-type>point</light-type>
    <name>cockpit light</name>
    <object-name>cockpit light volume</object-name>
    <nopreview />
    <position>
      <x>-1.50</x>
      <y> 0.00</y>
      <z> 0.62</z>
    </position>
    <ambient>
      <r>0.03</r>
      <g>0.03</g>
      <b>0.03</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>1.7</r>
      <g>1.7</g>
      <b>1.3</b>
      <a>1.0</a>
    </diffuse>
    <specular>
      <r>0.7</r>
      <g>0.7</g>
      <b>0.5</b>
      <a>1.0</a>
    </specular>
    <dim-factor>
      <property>electrical/outputs/cockpit-lights/brightness</property>
      <factor>1.0</factor>
    </dim-factor>
    <attenuation>
      <c>1.0</c>
      <l>3.0</l>
      <q>0.5</q>
    </attenuation>
    <near-m>0.05</near-m>
    <far-m> 1.30</far-m>
  </animation>

  <animation>
    <type>select</type>
    <object-name>cockpit light</object-name>
    <condition>
      <greater-than>
        <property>electrical/outputs/cockpit-lights/brightness</property>
        <value>0.05</value>
      </greater-than>
    </condition>
  </animation>

  <!-- SHADERS -->

  <effect>
    <inherits-from>Models/Effects/carpet</inherits-from>
    <object-name>floor</object-name>
  </effect>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/exterior-chrome.png</texture>
    <object-name>prop spinner</object-name>
    <object-name>prop spinner fast</object-name>
    <object-name>left main piston</object-name>
    <object-name>right main piston</object-name>
    <object-name>nose gear piston</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/exterior-chrome.png</texture>
    <object-name>yoke rod</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/glossy-black-plastic.png</texture>
    <object-name>left yoke</object-name>
    <object-name>right yoke</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/glass.png</texture>
    
    <object-name>windshield</object-name>
    <object-name>window 1</object-name>
    <object-name>window 3</object-name>
    <object-name>window 4</object-name>
    <object-name>window 5</object-name>
    <object-name>window 6</object-name>
    <object-name>door window</object-name>
    <object-name>left landing light cover</object-name>
    <object-name>right landing light cover</object-name>
    <object-name>left navigation light cover</object-name>
    <object-name>right navigation light cover</object-name>
    <object-name>tail beacon light cover</object-name>
  </animation>

  <effect>
    <inherits-from>Models/Effects/glass</inherits-from>
    
    <object-name>interior window</object-name>
  </effect>
  
  <!-- INTERNAL/EXTERNAL -->

  <animation>
    <type>select</type>
    <condition>
      <not>
        <property>/sim/current-view/internal</property>
      </not>
    </condition>
    <object-name>windshield</object-name>
    <object-name>window 1</object-name>
    <object-name>window 3</object-name>
    <object-name>window 4</object-name>
    <object-name>window 5</object-name>
    <object-name>window 6</object-name>
    <object-name>door window</object-name>
  </animation>

  <animation>
    <type>select</type>
    <condition>
      <property>/sim/current-view/internal</property>
    </condition>
    <object-name>interior window</object-name>
  </animation>

  <!-- fairings-->

  <animation>
    <type>select</type>
    <condition>
      <property>options/wheel-fairing</property>
    </condition>
    <object-name>main gear fairing</object-name>
    <object-name>left main fairing</object-name>
    <object-name>right main fairing</object-name>
    <object-name>nose gear fairing</object-name>
  </animation>

  <!-- COCKPIT -->

  <!-- yoke -->

  <animation>
    <type>select</type>
    <condition>
      <property>options/yoke-visible</property>
    </condition>
    <object-name>yoke rod</object-name>
    <object-name>left yoke</object-name>
    <object-name>right yoke</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>right yoke logo</object-name>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>yoke rod</object-name>
    <object-name>left yoke</object-name>
    <object-name>right yoke</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>right yoke logo</object-name>
    <property>controls/flight/elevator</property>
    <factor>-0.07</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>left yoke</object-name>
    <object-name>left yoke logo</object-name>
    <property>controls/flight/aileron</property>
    <factor>-90</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>1.85</x-m>
      <y-m>-0.23</y-m>
      <z-m>0.06</z-m>
    </center>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>right yoke</object-name>
    <object-name>right yoke logo</object-name>
    <property>controls/flight/aileron</property>
    <factor>-90</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>1.85</x-m>
      <y-m>0.23</y-m>
      <z-m>0.06</z-m>
    </center>
  </animation>

  <!-- flaps -->
  
  <animation>
    <type>rotate</type>
    <object-name>flap lever</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>45</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-1.510</x-m>
      <y-m> 0.000</y-m>
      <z-m>-0.409</z-m>
    </center>
  </animation>

  <!-- ENGINE -->
  
  <!-- prop -->

  <animation>
    <type>spin</type>
    <object-name>prop blade</object-name>
    <object-name>prop spinner</object-name>
    <property>engines/engine/rpm</property>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

  <!-- prop fade-->

  <animation>
    <type>select</type>
    <object-name>prop blade</object-name>
    <object-name>prop spinner</object-name>
    <condition>
      <less-than>
        <property>engines/engine/rpm</property>
        <value>400</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>prop disc front</object-name>
    <object-name>prop disc rear</object-name>
    <object-name>prop spinner fast</object-name>
    <condition>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>400</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>blend</type>
    <object-name>prop disc front</object-name>
    <object-name>prop disc rear</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>0.00015</factor>
    <max>1.0</max>
    <min>0.8</min>
  </animation>

  <!-- shader-->

  <!-- <effect> -->
  <!--   <inherits-from>Models/Effects/prop-disc</inherits-from> -->
  <!--   <object-name>prop disc</object-name> -->
  <!-- </effect> -->
  
  <!-- GEAR -->

  <!-- fairing select -->

  <animation>
    <type>pick</type>
    <object-name>left main fairing</object-name>
    <object-name>right main fairing</object-name>
    <object-name>nose gear fairing</object-name>

    <object-name>left main wheel</object-name>
    <object-name>right main wheel</object-name>
    <object-name>nose wheel</object-name>

    <!-- <action> -->
    <!--   <button>0</button> -->
    <!--   <repeatable>false</repeatable> -->
    <!--   <binding> -->
    <!--     <command>property-toggle</command> -->
    <!--     <property>options/wheel-fairing</property> -->
    <!--   </binding> -->
    <!-- </action> -->
  </animation>

  <!-- steering -->
  
  <animation>
    <type>rotate</type>
    <object-name>nose wheel</object-name>
    <object-name>nose gear fairing</object-name>
    <object-name>nose gear cylinder</object-name>
    <object-name>nose gear piston</object-name>
    <object-name>nose gear axle</object-name>
    <object-name>nose gear lower arm</object-name>
    <object-name>nose gear upper arm</object-name>
    <factor>-30</factor>
    <property>controls/flight/rudder</property>
    <axis>
      <x>0.2</x>
      <y>0</y>
      <z>0.8</z>
    </axis>
    <center>
      <x-m>-2.75</x-m>
      <y-m>0</y-m>
      <z-m>-1.02</z-m>
    </center>
  </animation>

  <!-- compression -->
  
  <animation>
    <type>translate</type>
    <object-name>nose wheel</object-name>
    <object-name>nose gear fairing</object-name>
    <object-name>nose gear piston</object-name>
    <object-name>nose gear axle</object-name>
    <object-name>nose gear lower arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <factor>0.3</factor>
    <axis>
      <x> 0.2</x>
      <y> 0.0</y>
      <z> 0.8</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>nose gear lower arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.5</ind>
        <dep>50</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>80</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-2.6303</x-m>
      <y-m>0</y-m>
      <z-m>-0.7504192</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>nose gear upper arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.6666</ind>
        <dep>-65</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-2.556</x-m>
      <y-m>0</y-m>
      <z-m>-0.541</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>left main gear lower arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>-60</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.7402972</x-m>
      <y-m>-1.38703</y-m>
      <z-m>-0.8650201</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-0.994004</y>
      <z>0.10934</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>left main gear upper arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.333</ind>
        <dep>25</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>65</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.728472</x-m>
      <y-m>-1.35513</y-m>
      <z-m>-0.5585561</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-0.994004</y>
      <z>0.10934</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>left main wheel</object-name>
    <object-name>left main piston</object-name>
    <object-name>left main fairing</object-name>
    <object-name>left main axle</object-name>
    <object-name>left main gear lower arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <factor>0.3</factor>
    <axis>
      <x>0</x>
      <y>0.10934</y>
      <z>0.994004</z>
    </axis>
  </animation>
  
  <animation>
