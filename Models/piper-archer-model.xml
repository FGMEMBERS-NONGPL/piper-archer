    <object-name>left strobe light flare</object-name>
    <object-name>right strobe light flare</object-name>
    <object-name>left navigation light flare</object-name>
    <object-name>right navigation light flare</object-name>
    <object-name>left landing light flare</object-name>
    <object-name>right landing light flare</object-name>
    <object-name>tail beacon light flare</object-name>
    <condition>
      <not>
        <property>/sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
  </animation>

  <!-- ALS shadow volume-->

  <animation>
    <type>select</type>
    <object-name>shadow volume</object-name>
    <condition>
      <and>
        <greater-than>
          <property>/sim/version/flightgear</property>
          <value>3.4.0</value>
        </greater-than>
        <not>
          <property>/sim/rendering/rembrandt/enabled</property>
        </not>
        <property>/sim/rendering/shaders/skydome</property>
        <property>/options/shadow</property>
      </and>
    </condition>
  </animation>

  <effect>
    <inherits-from>Effects/shadow-vol</inherits-from>
    <object-name>shadow volume</object-name>
  </effect>

  <animation>
    <type>noshadow</type>
    <object-name>shadow volume</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>shadow volume</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>-1.0</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>shadow volume</object-name>
    <property>/orientation/roll-deg</property>
    <factor>1.0</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  
</PropertyList>
