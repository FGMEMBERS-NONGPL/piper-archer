  </animation>

  <!-- AILERONS -->

  <animation>
    <type>rotate</type>
    <object-name>left aileron</object-name>
    <property>controls/flight/aileron</property>
    <factor>-25</factor>
    <axis>
      <x1-m>-0.244</x1-m>
      <y1-m>-2.703</y1-m>
      <z1-m>-0.134</z1-m>
      <x2-m>-0.482</x2-m>
      <y2-m>-5.438</y2-m>
      <z2-m> 0.175</z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>right aileron</object-name>
    <property>controls/flight/aileron</property>
    <factor>-25</factor>
    <axis>
      <x1-m>-0.244</x1-m>
      <y1-m> 2.703</y1-m>
      <z1-m>-0.134</z1-m>
