      </entry>
      <entry>
        <ind>0.333</ind>
        <dep>-25</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>-65</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.728472</x-m>
      <y-m>1.35513</y-m>
      <z-m>-0.5585561</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0.994004</y>
      <z>0.10934</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>right main wheel</object-name>
    <object-name>right main piston</object-name>
    <object-name>right main fairing</object-name>
    <object-name>right main axle</object-name>
    <object-name>right main gear lower arm</object-name>
    <property>gear/gear[2]/compression-ft</property>
    <factor>0.3</factor>
    <axis>
      <x>0</x>
      <y>-0.10934</y>
      <z>0.994004</z>
    </axis>
  </animation>

  <!-- LIVERY -->

  <animation>
    <type>material</type>
    <object-name>fuselage</object-name>
    <property-base>sim/model/livery/</property-base>
    <texture-prop>texture</texture-prop>
    <texture>fuselage-n301dp.png</texture>
  </animation>

  <!-- DOOR -->

  <animation>
    <type>rotate</type>
    <object-name>door</object-name>
    <object-name>door window</object-name>
    <property>controls/door</property>
    <factor>90</factor>
    <axis>
      <x1-m>-1.91226</x1-m>
      <y1-m>0.5664281</y1-m>
      <z1-m>-0.189439</z1-m>
      <x2-m>-1.91022</x2-m>
      <y2-m>0.5463464</y2-m>
      <z2-m>0.0711347</z2-m>
