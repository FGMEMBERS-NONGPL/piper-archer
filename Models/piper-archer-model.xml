        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>50</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.7402972</x-m>
      <y-m>1.38703</y-m>
      <z-m>-0.8650201</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0.994004</y>
