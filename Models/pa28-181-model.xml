<?xml version="1.0"?>

<PropertyList>

  <!-- Export X/Y -->
  <path>Models/pa28-181.ac</path>

  <!-- REMBRANDT -->

  <animation>
    <type>select</type>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>shadow catcher</object-name>
  </animation>

  <effect>
    <condition>
      <property>/sim/rendering/rembrandt/enabled</property>
    </condition>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>prop disc</object-name>
  </effect>

  <!-- COCKPIT -->

  <effect>
    <inherits-from>Aircraft/pa28-181/Models/Effects/brown-plastic</inherits-from>
    <object-name>dashboard</object-name>
    <object-name>throttle quadrant</object-name>
  </effect>

  <model>
    <name>airspeed</name>
    <path>Aircraft/pa28-181/Instruments/airspeed/airspeed.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.31</y-m>
      <z-m>0.25</z-m>
    </offsets>
  </model>

  <model>
    <name>attitude</name>
    <path>Aircraft/pa28-181/Instruments/attitude/attitude.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.22</y-m>
      <z-m>0.25</z-m>
    </offsets>
  </model>

  <model>
    <name>altimeter</name>
    <path>Aircraft/pa28-181/Instruments/altimeter/altimeter.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.13</y-m>
      <z-m>0.25</z-m>
    </offsets>
  </model>

  <model>
    <name>bank</name>
    <path>Aircraft/pa28-181/Instruments/bank/bank.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.31</y-m>
      <z-m>0.16</z-m>
    </offsets>
  </model>

  <model>
    <name>compass</name>
    <path>Aircraft/pa28-181/Instruments/compass/compass.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.22</y-m>
      <z-m> 0.16</z-m>
    </offsets>
  </model>

  <model>
    <name>vsi</name>
    <path>Aircraft/pa28-181/Instruments/vsi/vsi.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.13</y-m>
      <z-m> 0.16</z-m>
    </offsets>
  </model>

  <!-- fuel row -->

  <model>
    <name>placeholder</name>
    <path>Aircraft/pa28-181/Instruments/placeholder/placeholder.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.19</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>fuel-left</name>
    <path>Aircraft/pa28-181/Instruments/fuel/fuel-left.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.15</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>fuel-pressure</name>
    <path>Aircraft/pa28-181/Instruments/fuel-pressure/fuel-pressure.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.11</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>fuel-right</name>
    <path>Aircraft/pa28-181/Instruments/fuel/fuel-right.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.07</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>placeholder</name>
    <path>Aircraft/pa28-181/Instruments/placeholder/placeholder.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.03</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <!-- battery row -->

  <model>
    <name>placeholder</name>
    <path>Aircraft/pa28-181/Instruments/placeholder/placeholder.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.43</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>battery</name>
    <path>Aircraft/pa28-181/Instruments/battery/battery.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.39</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>placeholder</name>
    <path>Aircraft/pa28-181/Instruments/placeholder/placeholder.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.35</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <model>
    <name>placeholder</name>
    <path>Aircraft/pa28-181/Instruments/placeholder/placeholder.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.96</x-m>
      <y-m>-0.31</y-m>
      <z-m> 0.07</z-m>
    </offsets>
  </model>

  <!-- Switches -->

  <model>
    <name>switches</name>
    <path>Aircraft/pa28-181/Instruments/switches/switches.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.961</x-m>
      <y-m> 0.094</y-m>
      <z-m> 0.05</z-m>
    </offsets>
  </model>

  <!-- engine instruments -->

  <model>
    <name>tachometer</name>
    <path>Aircraft/pa28-181/Instruments/tachometer/tachometer.xml</path>
    <offsets>
      <pitch-deg>90</pitch-deg>
      <roll-deg>180</roll-deg>
      <x-m>-1.95</x-m>
      <y-m>-0.06</y-m>
      <z-m>-0.04</z-m>
    </offsets>
  </model>


  <!-- COCKPIT LIGHTING -->

  <animation>
    <type>material</type>
    <condition>
      <not>
        <property>/sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <object-name>dashboard</object-name>
    <object-name>glareshield</object-name>
    <object-name>yoke mount trim</object-name>
    <object-name>engine gauge trim</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>right yoke logo</object-name>
    <object-name>engine gauge trim</object-name>
    <object-name>throttle quadrant trim</object-name>
    <object-name>throttle quadrant</object-name>
    <object-name>knee padding</object-name>

    <!-- <object-name>yoke rod</object-name> -->
    <!-- <object-name>yoke rod gasket</object-name> -->
    <!-- <object-name>left yoke</object-name> -->
    <!-- <object-name>right yoke</object-name> -->
    <!-- <object-name>throttle lever</object-name> -->
    <emission>
      <red>0.03</red>
      <green>0.02</green>
      <blue>0.02</blue>
      <factor-prop>electrical/outputs/cockpit-lights/brightness</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>light</type>
    <light-type>point</light-type>
    <name>cockpit light</name>
    <object-name>cockpit light volume</object-name>
    <nopreview />
    <position>
      <x>-1.50</x>
      <y> 0.00</y>
      <z> 0.62</z>
    </position>
    <ambient>
      <r>0.03</r>
      <g>0.03</g>
      <b>0.03</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>0.7</r>
      <g>0.7</g>
      <b>0.6</b>
      <a>1.0</a>
    </diffuse>
    <specular>
      <r>0.7</r>
      <g>0.7</g>
      <b>0.7</b>
      <a>1.0</a>
    </specular>
    <dim-factor>
      <property>electrical/outputs/cockpit-lights/brightness</property>
      <factor>0.05</factor>
    </dim-factor>
    <attenuation>
      <c>1.0</c>
      <l>1.0</l>
      <q>1.0</q>
    </attenuation>
    <near-m>0.05</near-m>
    <far-m> 1.10</far-m>
  </animation>

  <animation>
    <type>select</type>
    <object-name>cockpit light</object-name>
    <condition>
      <and>
        <property>/sim/rendering/rembrandt/enabled</property>
        <greater-than>
          <property>/sim/rendering/shaders/lights</property>
          <value>2.0</value>
        </greater-than>
        <greater-than>
          <property>electrical/outputs/cockpit-lights/brightness</property>
          <value>0.05</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <!-- SHADERS -->

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/exterior-chrome.png</texture>
    <object-name>prop spinner</object-name>
    <object-name>left main piston</object-name>
    <object-name>right main piston</object-name>
    <object-name>nose gear piston</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/interior-chrome.png</texture>
    <object-name>yoke rod</object-name>
    <object-name>yoke rod gasket</object-name>
    <object-name>flap lever</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/glossy-black-plastic.png</texture>
    <object-name>throttle lever</object-name>
    <object-name>left yoke</object-name>
    <object-name>right yoke</object-name>
  </animation>

  <!-- Cannot have duplicate shaders; either rembrandt or chromey windows -->
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/pa28-181/Models/Effects/glass.png</texture>
    <object-name>windshield</object-name>
    <object-name>side windows</object-name>
    <object-name>door window</object-name>
  </animation>

  <!-- INTERNAL/EXTERNAL -->

  <animation>
    <type>select</type>
    <condition>
      <not>
        <property>sim/current-view/internal</property>
      </not>
    </condition>
    <object-name>windshield</object-name>
    <object-name>side windows</object-name>
    <object-name>door window</object-name>
  </animation>

  <!-- fairings-->

  <animation>
    <type>select</type>
    <condition>
      <property>options/wheel-fairing</property>
    </condition>
    <object-name>main gear fairing</object-name>
    <object-name>left main fairing</object-name>
    <object-name>right main fairing</object-name>
    <object-name>nose gear fairing</object-name>
  </animation>

  <!-- COCKPIT -->

  <!-- yoke -->

  <animation>
    <type>translate</type>
    <object-name>yoke rod</object-name>
    <object-name>left yoke</object-name>
    <object-name>right yoke</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>right yoke logo</object-name>
    <property>controls/flight/elevator</property>
    <factor>-0.07</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>left yoke</object-name>
    <object-name>left yoke logo</object-name>
    <property>controls/flight/aileron</property>
    <factor>-90</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>1.85</x-m>
      <y-m>-0.23</y-m>
      <z-m>0.06</z-m>
    </center>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>right yoke</object-name>
    <object-name>right yoke logo</object-name>
    <property>controls/flight/aileron</property>
    <factor>-90</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>1.85</x-m>
      <y-m>0.23</y-m>
      <z-m>0.06</z-m>
    </center>
  </animation>

  <!-- flaps -->
  
  <animation>
    <type>rotate</type>
    <object-name>flap lever</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>45</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-1.56</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.409</z-m>
    </center>
  </animation>

  <!-- throttle lever -->
  
  <animation>
    <type>knob</type>
    <object-name>throttle lever</object-name>
    <property>controls/engines/engine/throttle</property>
    <interpolation>
      <entry>
	<ind>0</ind>
	<dep>0</dep>
      </entry>
      <entry>
	<ind>1</ind>
	<dep>-60</dep>
      </entry>
    </interpolation>
    <drag-direction>vertical</drag-direction>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-1.95</x-m>
      <y-m>0.3</y-m>
      <z-m>-0.095</z-m>
    </center>

    <action>
      <binding>
	<command>property-adjust</command>
	<property>controls/engines/engine/throttle</property>
	<factor>0.02</factor>
	<min>0.0</min>
	<max>1.0</max>                
	<wrap>false</wrap>
      </binding>
    </action>

    <hovered>
      <binding>
	<command>set-tooltip</command>
	<tooltip-id>throttle</tooltip-id>
	<label>Throttle: %d%%</label>
	<mapping>percent</mapping>
	<property>controls/engines/engine/throttle</property>
      </binding>
    </hovered>

  </animation>

  <!-- ENGINE -->
  
  <!-- prop -->

  <animation>
    <type>spin</type>
    <object-name>prop blade</object-name>
    <property>engines/engine/rpm</property>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
  </animation>

  <!-- prop fade-->

  <animation>
    <type>select</type>
    <object-name>prop blade</object-name>
    <condition>
      <less-than>
        <property>engines/engine/rpm</property>
        <value>400</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>prop disc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>400</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>blend</type>
    <object-name>prop disc</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor>0.0003</factor>
    <max>1.0</max>
    <min>0.0</min>
  </animation>
  
  <!-- GEAR -->

  <!-- fairing select -->

  <animation>
    <type>pick</type>
    <object-name>left main fairing</object-name>
    <object-name>right main fairing</object-name>
    <object-name>nose gear fairing</object-name>

    <object-name>left main wheel</object-name>
    <object-name>right main wheel</object-name>
    <object-name>nose wheel</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>options/wheel-fairing</property>
      </binding>
    </action>
  </animation>

  <!-- steering -->
  
  <animation>
    <type>rotate</type>
    <object-name>nose wheel</object-name>
    <object-name>nose gear fairing</object-name>
    <object-name>nose gear cylinder</object-name>
    <object-name>nose gear piston</object-name>
    <object-name>nose gear axle</object-name>
    <object-name>nose gear lower arm</object-name>
    <object-name>nose gear upper arm</object-name>
    <factor>-30</factor>
    <property>controls/flight/rudder</property>
    <axis>
      <x>0.2</x>
      <y>0</y>
      <z>0.8</z>
    </axis>
    <center>
      <x-m>-2.75</x-m>
      <y-m>0</y-m>
      <z-m>-1.02</z-m>
    </center>
  </animation>

  <!-- compression -->
  
  <animation>
    <type>rotate</type>
    <object-name>nose gear lower arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>70</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-2.6303</x-m>
      <y-m>0</y-m>
      <z-m>-0.7504192</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>nose gear upper arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.6666</ind>
        <dep>-80</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-2.556</x-m>
      <y-m>0</y-m>
      <z-m>-0.541</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>nose wheel</object-name>
    <object-name>nose gear fairing</object-name>
    <object-name>nose gear piston</object-name>
    <object-name>nose gear axle</object-name>
    <object-name>nose gear lower arm</object-name>
    <property>gear/gear[0]/compression-ft</property>
    <factor>0.3</factor>
    <axis>
      <x> 0.2</x>
      <y> 0.0</y>
      <z> 0.8</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>left main gear lower arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>-40</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.7402972</x-m>
      <y-m>-1.38703</y-m>
      <z-m>-0.8650201</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-0.994004</y>
      <z>0.10934</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>left main gear upper arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>0.333</ind>
        <dep>25</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>65</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.728472</x-m>
      <y-m>-1.35513</y-m>
      <z-m>-0.5585561</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-0.994004</y>
      <z>0.10934</z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>left main wheel</object-name>
    <object-name>left main piston</object-name>
    <object-name>left main fairing</object-name>
    <object-name>left main axle</object-name>
    <object-name>left main gear lower arm</object-name>
    <property>gear/gear[1]/compression-ft</property>
    <factor>0.3</factor>
    <axis>
      <x>0</x>
      <y>0.10934</y>
      <z>0.994004</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>right main gear lower arm</object-name>
    <property>gear/gear[2]/compression-ft</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>40</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-0.7402972</x-m>
      <y-m>1.38703</y-m>
      <z-m>-0.8650201</z-m>
    </center>
