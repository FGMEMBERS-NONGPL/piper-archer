  <!-- Shaders -->

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/exterior-chrome.png</texture>
    <object-name>yoke rod gasket</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Models/Effects/glossy-black-plastic.png</texture>
    <object-name>throttle lever</object-name>
  </animation>

  <!-- Lighting -->

  <animation>
    <type>material</type>
    <condition>
      <not>
        <property>/sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <object-name>dashboard</object-name>
    <object-name>cockpit registration</object-name>
    <object-name>yoke mount trim</object-name>
    <object-name>throttle quadrant trim</object-name>
    <object-name>throttle quadrant</object-name>
    <object-name>knee padding</object-name>
    <emission>
      <red>0.3</red>
      <green>0.2</green>
      <blue>0.2</blue>
      <factor-prop>electrical/outputs/cockpit-lights/brightness</factor-prop>
    </emission>
  </animation>

  <!-- Range -->

  <animation>
    <type>range</type>
    <object-name>fuel selector</object-name>
    <object-name>left yoke</object-name>
    <object-name>left yoke logo</object-name>
    <object-name>left yoke</object-name>
    <object-name>right yoke logo</object-name>
    <object-name>yoke rod trim</object-name>
    <min-m>   0</min-m>
    <min-m>1000</min-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>clock</object-name>
    <object-name>airspeed</object-name>
    <object-name>attitude</object-name>
    <object-name>altimeter</object-name>
    <object-name>bank</object-name>
    <object-name>compass</object-name>
    <object-name>vsi</object-name>
    <object-name>placeholder</object-name>
    <object-name>fuel-left</object-name>
    <object-name>fuel-pressure</object-name>
    <object-name>fuel-right</object-name>
    <object-name>oil-pressure</object-name>
    <object-name>oil-temperature</object-name>
    <object-name>switches</object-name>
    <object-name>key</object-name>
    <object-name>tachometer</object-name>
    <min-m>   0</min-m>
    <min-m> 500</min-m>
  </animation>

  <!-- Yoke hiding -->

  <animation>
    <type>pick</type>
    <object-name>yoke mount trim</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>options/yoke-visible</property>
      </binding>
    </action>
  </animation>

  <!-- throttle lever -->
  
  <animation>
    <type>knob</type>
    <object-name>throttle lever</object-name>
    <property>controls/engines/engine/throttle</property>
    <interpolation>
      <entry>
	<ind>0</ind>
	<dep>0</dep>
      </entry>
      <entry>
	<ind>1</ind>
	<dep>-60</dep>
      </entry>
    </interpolation>
    <drag-direction>vertical</drag-direction>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-1.95</x-m>
      <y-m>0.3</y-m>
      <z-m>-0.095</z-m>
    </center>

    <action>
      <binding>
	<command>property-adjust</command>
	<property>controls/engines/engine/throttle</property>
	<factor>0.02</factor>
	<min>0.0</min>
	<max>1.0</max>                
	<wrap>false</wrap>
      </binding>
    </action>

    <hovered>
      <binding>
	<command>set-tooltip</command>
	<tooltip-id>throttle</tooltip-id>
	<label>Throttle: %d%%</label>
	<mapping>percent</mapping>
	<property>controls/engines/engine/throttle</property>
      </binding>
    </hovered>

  </animation>

  <!-- mixture lever -->
  
  <animation>
    <type>knob</type>
    <object-name>mixture lever</object-name>
    <property>controls/engines/engine/mixture</property>
    <interpolation>
      <entry>
	<ind>0</ind>
	<dep>0</dep>
      </entry>
      <entry>
	<ind>1</ind>
	<dep>-60</dep>
      </entry>
    </interpolation>
    <drag-direction>vertical</drag-direction>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-1.950</x-m>
      <y-m> 0.000</y-m>
      <z-m>-0.095</z-m>
    </center>

    <action>
      <binding>
	<command>property-adjust</command>
	<property>controls/engines/engine/mixture</property>
	<factor>0.02</factor>
	<min>0.0</min>
	<max>1.0</max>                
	<wrap>false</wrap>
      </binding>
    </action>

    <hovered>
      <binding>
	<command>set-tooltip</command>
	<tooltip-id>mixture</tooltip-id>
	<label>Mixture: %d%%</label>
	<mapping>percent</mapping>
	<property>controls/engines/engine/mixture</property>
      </binding>
    </hovered>

  </animation>

</PropertyList>
