<?xml version="1.0"?>

<PropertyList>

  <failure>
    <name>engine fail during takeoff run</name>
    <trigger>
      <condition>
        <property>/velocities/airspeed-kt</property>
        <comparison>greater-than</comparison>
        <offset>40</offset>
      </condition>
    </trigger>
    <start>
      <condition>
        <property>/velocities/airspeed-kt</property>
        <comparison>greater-than</comparison>
        <offset>40</offset>
      </condition>
    </start>
    <fail>engine</fail>
  </failure>
  
</PropertyList>
