<?xml version="1.0"?>

<PropertyList>

  <source>
    <name>battery</name>
    <type>battery</type>
    <capacity type="double">200</capacity>
    <empty-voltage type="double">8</empty-voltage>
    <full-voltage type="double">12</full-voltage>
    <prop type="string">/electrical/battery</prop>
    <output>primary bus</output>
  </source>

  <source>
    <name>alternator</name>
    <type>alternator</type>
    <voltage type="double">14</voltage>
    <rpm-source>/engines/engine[0]/rpm</rpm-source>
    <min-rpm type="double">200</min-rpm>
    <max-rpm type="double">2400</max-rpm>
    <power type="double">100</power> <!-- 100 watt alternator-->
    <prop type="string">/electrical/alternator</prop>
    <output>battery</output>
  </source>
  
  <component>
    <name>fuel pump</name>
    <type>needs-power</type>
    <min-voltage type="double">8</min-voltage>
    <nominal-voltage type="double">8</nominal-voltage>
    <prop type="string">/electrical/outputs/fuel-pump</prop>
    <switch>/controls/switches/fuel</switch>
    <input>battery</input>
  </component>
  
</PropertyList>
